@model IEnumerable<string>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="number" name="score" />
    <button type="submit">Kaydet</button>
    
</form>

@foreach (var item in Model)
{
    <ul>
        <li>
            <a asp-action="deleteItem" asp-route-name="@item.ToString()">Sil</a> 
            @item.ToString();
        </li>
    </ul>
}
